{
  "db_name": "videos",
  "db_file": "data/videos.db",
  "version": 2,
  "maps": {
    "users/.+/data.json": {
      "to_table": ["videos","played","magnet"]
    }
  },
  "tables": {
    "videos": {
      "cols": [
        ["id", "INTEGER PRIMARY KEY NOT NULL DEFAULT 0"],
        ["time", "INTEGER NOT NULL DEFAULT 1"],
        ["high", "BOOLEAN NOT NULL DEFAULT 1"],
        ["name", "VARCHAR(666) NOT NULL DEFAULT ''"],
        ["path", "VARCHAR(666) NOT NULL DEFAULT ''"],
        ["count", "INTEGER NOT NULL DEFAULT 0"],
        ["image", "VARCHAR(666) NOT NULL DEFAULT ''"],
        ["json_id", "INTEGER REFERENCES json (json_id) DEFAULT 0"]
      ],
      "indexes": [
        "CREATE UNIQUE INDEX videos_key ON videos(id, json_id)",
        "CREATE UNIQUE INDEX videos_id ON videos(id)"
      ],
      "schema_changed": 1
    },
    "played": {
      "cols": [
        ["video", "INTEGER NOT NULL DEFAULT 32767"],
        ["json_id", "INTEGER REFERENCES json (json_id) DEFAULT 0"]
      ],
      "indexes": [
        "CREATE UNIQUE INDEX played_key ON played(video, json_id)",
        "CREATE INDEX played_video ON played(video)"
      ],
      "schema_changed": 1
    },
    "magnet": {
      "cols": [
        ["id", "INTEGER NOT NULL DEFAULT 0"],
        ["date", "INTEGER NOT NULL DEFAULT 0"],
        ["magnet", "VARCHAR(666) NOT NULL DEFAULT ''"],
        ["json_id", "INTEGER REFERENCES json (json_id) DEFAULT 0"]
      ],
      "indexes": [
        "CREATE INDEX magnet_key ON magnet(id, json_id)"
      ],
      "schema_changed": 1
    }
  }
}
