      {
        "href": "/donate/",
        "type": "text/html"
      }
    ],
    "icon": [
      {
        "href": "pow_icon.svg",
        "type": "image/svg",
        "sizes": "any",
        "lazyload": "1"
      }
    ],
    "author": [
      {
        "href": "/humans.txt",
        "type": "text/plain"
      }
    ],
    "manifest": [
      {
        "href": "/lucid00.webmanifest"
      }
    ],
    "search": [
      {
        "href": "/opensearch.xml",
        "type": "application/opensearchdescription+xml",
        "title": "Lucid00 Search"
      }
    ],
    "serviceworker": [
      {
        "href": "/serviceworker.js",
        "scope": "/"
      }
    ],
    "preload": [
      {
        "href": "/pow_icon.svg",
        "as": "image"
      }
    ],
    "subresource": [
      {
        "href": "/serviceworker.js"
      }
    ]
  }
}