{
    "db_name": "Shortened",
    "db_file": "data/shortened.db",
    "version": 2,
    "maps": {
        "users/.+/data.json": {
            "to_table": [ "shortened" ]
        },
        "users/.+/content.json": {
            "to_json_table": [ "cert_user_id" ],
            "file_name": "data.json"
        }
    },
    "tables": {
        "json": {
            "cols": [
                ["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
                ["directory", "TEXT"],
                ["file_name", "TEXT"],
                ["cert_user_id", "TEXT"]
            ],
            "indexes": ["CREATE UNIQUE INDEX path ON json(directory, file_name)"],
            "schema_changed": 10
        },
        "shortened": {
            "cols": [
                ["URL", "TEXT"],
                ["CODE", "TEXT"],
                ["json_id", "INTEGER REFERENCES json (json_id)"]
            ],
            "indexes": ["CREATE UNIQUE INDEX shortened_key ON shortened(json_id, CODE)"],
            "schema_changed": 10
        }
    }
}