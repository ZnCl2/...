{
	"db_name": "StreamZ",
	"db_file": "data/StreamZ.db",
	"version": 2,
	"maps": {
		"users/.+/content.json": {
			"to_json_table": ["cert_user_id"],
			"file_name": "data.json"
		},
		"users/.+/data.json": {
			"to_table": ["videos"]
		}
	},
	"tables": {
		"json": {
			"cols": [
				["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
				["directory", "TEXT"],
				["file_name", "TEXT"],
				["cert_user_id", "TEXT"]
			],
			"indexes": ["CREATE UNIQUE INDEX path ON json(directory, file_name)"],
			"schema_changed": 1
		},
		"videos": {
			"cols": [
				["title", "TEXT"],
				["description", "TEXT"],
				["id", "TEXT"],
				["last_update", "INTEGER"],
				["json_id", "INTEGER"]
			],
			"indexes": ["CREATE UNIQUE INDEX video ON videos(id, json_id)"],
			"schema_changed": 2
		}
	}
}