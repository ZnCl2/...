{
  "db_name": "Heroes",
  "db_file": "data/heroes.db",
  "version": 2,
  "maps": {
    "users/.+/data.json": {
      "to_table": [ "heroes" ]
    },
    "users/.+/content.json": {
      "to_json_table": [ "cert_user_id" ],
      "file_name": "data.json"
    }
  },
  "tables": {
    "json": {
      "cols": [
        [
          "json_id",
          "INTEGER PRIMARY KEY AUTOINCREMENT"
        ],
        [
          "directory",
          "TEXT"
        ],
        [
          "file_name",
          "TEXT"
        ],
        [
          "cert_user_id",
          "TEXT"
        ]
      ],
      "indexes": [
        "CREATE UNIQUE INDEX path ON json(directory, file_name)"
      ],
      "schema_changed": 1
    },
    "heroes": {
      "cols": [
        [
          "name",
          "TEXT"
        ],
        [
          "created_at",
          "INTEGER"
        ],
        [
          "json_id",
          "INTEGER REFERENCES json(json_id)"
        ]
      ],
      "indexes": [
        "CREATE UNIQUE INDEX heroes_key ON heroes(json_id, created_at)"
      ],
      "schema_changed": 1
    }
  }
}
