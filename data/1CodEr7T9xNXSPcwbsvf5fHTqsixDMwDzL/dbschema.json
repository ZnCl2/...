{
    "db_name": "CodeR",
    "db_file": "data/users/coder.db",
    "version": 2,
    "maps": {
        ".+/data.json": {
            "to_table": [
                {
                    "node": "code",
                    "table": "code"
                }
            ]
        },
        ".+/content.json": {
            "to_keyvalue": ["cert_user_id"]
        }
    },
    "tables": {
        "code": {
            "cols": [
                ["id", "INTEGER"],
                ["json_id", "INTEGER REFERENCES json (json_id)"],
                ["html", "TEXT"],
                ["js", "TEXT"],
                ["css", "TEXT"],
                ["js_mode", "TEXT"],
                ["css_mode", "TEXT"],
                ["library", "TEXT"],
                ["title", "TEXT"],
                ["description", "TEXT"],
                ["added", "DATETIME"]
            ],
            "indexes": ["CREATE UNIQUE INDEX code_key ON code(id, json_id)"],
            "schema_changed": 2
        }
    }
}