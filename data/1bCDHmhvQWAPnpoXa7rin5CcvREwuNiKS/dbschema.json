{
	"db_name": "Plus1s",
	"db_file": "data/plus.db",
	"version": 2,
	"maps": {
		"users/.+/data.json": {
			"to_table": ["pluses"]
		},
		"users/.+/content.json": {
			"to_keyvalue": ["cert_user_id"]
		}
	},
	"tables": {
		"pluses": {
			"cols": [
				["seconds", "INTEGER"],
				["json_id", "INTEGER REFERENCES json (json_id)"]
			],
			"indexes": ["CREATE UNIQUE INDEX haha_key ON pluses(json_id)"],
			"schema_changed": 1
		}
	}
}
