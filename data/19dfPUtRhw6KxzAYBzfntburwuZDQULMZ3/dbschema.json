{
	"db_name": "SadMatadata",
	"db_file": "data/SadMetadata.db",
	"version": 2,
	"maps": {
		"Metadata/files/GalleryMetadataZN.*.json": {
			"to_table": [
				{"node": "metadata", "table": "metadata", "key_col":"ID"}
			]
		},
		"Tags/files/GalleryTagsZN.*.json": {
			"to_table": [
				{"node": "tags", "table": "tags", "key_col":"MetadataID"}
			]
		}
	},
	"tables": {
		"metadata": {
			"cols": [
				["title", "TEXT"],
				["pages", "INTEGER"],
				["uploader", "TEXT"],
				["href", "TEXT"],
				["type", "TEXT"],
				["uploadDate", "INTEGER"],
				["SET_INFO", "TEXT"],
				["ID", "TEXT"],
				["PAGE", "INTEGER"],
				["IS_DATA_MISSING", "INTEGER"],
				["json_id", "INTEGER REFERENCES json (json_id)"]
			],
			"indexes": [],
			"schema_changed": 2
		},
		"tags": {
			"cols": [
				["MetadataID", "TEXT"],
				["namespace", "TEXT"],
				["name", "TEXT"],
				["power", "TEXT"],
				["json_id", "INTEGER REFERENCES json (json_id)"]
			],
			"indexes": [],
			"schema_changed": 2
		}
	}
}