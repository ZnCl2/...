{
    "db_name": "ZeroMarket",
    "db_file": "data/zeromarket.db",
    "version": 2,
    "maps": {
        "users/.+/data.json": {
            "to_table": [ "market" ]
        },
        "users/.+/content.json": {
            "to_json_table": [ "cert_user_id" ],
            "file_name": "data.json"
        }
    },
    "tables": {
        "json": {
            "cols": [
                ["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
                ["directory", "TEXT"],
                ["file_name", "TEXT"],
                ["cert_user_id", "TEXT"]
            ],
            "indexes": ["CREATE UNIQUE INDEX path ON json(directory, file_name)"],
            "schema_changed": 10
        },
        "market": {
            "cols": [
				["title", "TEXT"],
				["date_added", "INTEGER"],
				["json_id", "INTEGER REFERENCES json (json_id)"],
				["img1","TEXT"],
				["price","TEXT"],
				["category","TEXT"],
				["details","TEXT"],
				["deliveryFrom","TEXT"],
				["productType","TEXT"]
            ],
            "indexes": ["CREATE UNIQUE INDEX market_key ON market(json_id, date_added)"],
            "schema_changed": 10
        }
    }
}