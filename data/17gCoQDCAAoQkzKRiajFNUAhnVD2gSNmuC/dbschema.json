{
    "db_name": "Millchan",
    "db_file": "merged-Millchan/millchan.db",
    "version": 3,
    "maps": {
        ".+/data/users/.+/data.json": {
            "to_table": [ "posts", "modlogs" ]
        },
        ".+/data/users/.+/content.json": {
			"to_keyvalue": [ "cert_user_id" ]
		}
    },
    "tables": {
        "posts": {
            "cols": [
                ["id", "TEXT"],
                ["thread", "INTEGER"],
                ["subject", "TEXT"],
                ["body", "TEXT"],
                ["username", "TEXT"],
                ["time", "INTEGER"],
                ["files", "TEXT"],
                ["last_edited","INTEGER"],
                ["capcode","INTEGER"],
                ["json_id", "INTEGER REFERENCES json (json_id)"]
            ],
            "indexes": [
		"CREATE UNIQUE INDEX uuid ON posts(id)",
		"CREATE UNIQUE INDEX post_id ON posts(id,json_id)"
		],
            "schema_changed": 14
        },
	"modlogs": {
		"cols": [
			["uri", "TEXT"],
			["time", "INTEGER"],
			["action", "INTEGER"],
			["info", "TEXT"],
			["json_id", "INTEGER REFERENCES json (json_id)"]
			
		],
		"indexes": ["CREATE UNIQUE INDEX log_id ON modlogs(uri,action,info,json_id)"],
		"schema_changed": 12
	},
	"json": {
		"cols": [
			["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
			["site", "TEXT"],
			["directory", "TEXT"],
			["file_name", "TEXT"]
		],
		"indexes": ["CREATE UNIQUE INDEX path ON json(directory, site, file_name)"],
		"schema_changed": 12
	}
    }
}
