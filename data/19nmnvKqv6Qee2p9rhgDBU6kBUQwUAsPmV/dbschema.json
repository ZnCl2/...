{
	"db_name": "ZeroSites",
	"db_file": "data/zerosites.db",
	"version": 2,
	"maps": {
		"sites.json": {
			"to_table": [ "site" ]
		}

	},
	"tables": {
		"site": {
			"cols": [
				["site_id", "INTEGER"],
				["category", "TEXT"],
				["title", "TEXT"],
				["description", "TEXT"],
				["address", "TEXT"],
				["language", "TEXT"],
				["tags", "TEXT"],
				["peers", "INTEGER"],
				["date_modified", "DATETIME"],
				["date_added", "DATETIME"],
				["sort", "INTEGER"],
				["json_id", "INTEGER REFERENCES json (json_id)"]
			],
			"indexes": ["CREATE UNIQUE INDEX site_key ON site(json_id, site_id)", "CREATE INDEX site_sort ON site(sort)"],
			"schema_changed": 5
		}
	}
}