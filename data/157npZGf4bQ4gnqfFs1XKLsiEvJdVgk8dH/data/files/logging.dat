[loggers]
keys=root,syslog

[handlers]
keys=syslog

[formatters]
keys=syslog

[logger_root]
level=WARNING
handlers=syslog

[logger_syslog]
level=WARNING
handlers=syslog
qualname=default
propagate=0

[handler_syslog]
class=logging.NullHandler
formatter=syslog
level=WARNING
args=()

[formatter_syslog]
format=%(asctime)s %(threadName)s %(filename)s@%(lineno)d %(message)s
datefmt=%F %T
