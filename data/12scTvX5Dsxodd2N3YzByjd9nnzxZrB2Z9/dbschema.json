{
"db_name": "test",
"db_file": "data/users/test.db",
"version": 2,
"maps":{
	"users/.+/data.json":{
		"to_table":["test"]
	},
	"users/.+/content.json":{
		"to_keyvalue": ["cert_user_id"]
	}
},
"tables":{
	"test":{
		"cols":[
			["id", "INTEGER"],
			["name", "TEXT"],
			["json_id", "INTEGER REFERENCES json (json_id)"]
			
		],
		"indexes":["create UNIQE INDEX id ON test(id)"],
		"schema_changed":1
	
	}
	
}

}