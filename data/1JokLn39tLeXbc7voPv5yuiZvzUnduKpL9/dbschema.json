{
	"db_name": "Gziptest",
	"db_file": "data/gziptest.db",
	"version": 2,
	"maps": {
        "stats.*json": {
        	"to_table": [
        		"site_stat"
        	]
        }
	},
	"tables": {
		"site_stat": {
			"cols": [
				["site_uri", "TEXT"],
				["peers", "INTEGER"],
				["date_updated", "DATETIME"],
				["json_id", "INTEGER REFERENCES json (json_id)"]
			],
			"indexes":["CREATE INDEX site_stat_key ON site_stat(json_id)", "CREATE UNIQUE INDEX site_stat_uri ON site_stat(site_uri)"],
			"schema_changed": 10
		}
	}
}